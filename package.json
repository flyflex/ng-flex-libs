{
  "name": "ngflex-libs",
  "version": "3.4.0",
  "license": "MIT",
  "description": "NGFlex ngrx, firebase and js libraries",
  "packageManager": "yarn@3.5.1",
  "repository": {
    "type": "github",
    "url": "https://github.com/flyflex/ngflex-libs"
  },
  "author": "FlyFlex",
  "scripts": {
    "build:ngrx-firebase": "nx run ngrx-firebase:build",
    "build": "nx run-many --target=build --all",
    "lint": "nx run-many --target=lint --all",
    "test:affected": "nx affected --target=test --coverage",
    "test": "nx run-many --target=test --all --coverage",
    "test:watch": "jest --watch",
    "publish:ngrx-firebase": "nx run ngrx-firebase:publish --tag=next --otp",
    "publish:merge-coverage-reports": "nx run merge-coverage-reports:publish --tag=next --otp",
    "verif:all": "yarn build && yarn lint && yarn test",
    "merge-reports": "nx run merge-coverage-reports:merge"
  },
  "dependencies": {
    "minimist": "1.2.8",
    "tslib": "2.5.2"
  },
  "devDependencies": {
    "@angular/fire": "7.5.0",
    "@firebase/app-types": "0.9.0",
    "@ngrx/store": "16.0.0",
    "@nx/eslint-plugin": "16.2.2",
    "@nx/jest": "16.2.2",
    "@nx/js": "16.2.2",
    "@nx/linter": "16.2.2",
    "@nx/workspace": "16.2.2",
    "@types/fs-extra": "11.0.1",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.1",
    "@types/minimist": "1.2.2",
    "@types/node": "18.16.8",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "eslint": "8.41.0",
    "firebase": "9.22.0",
    "fs-extra": "11.1.1",
    "glob": "10.2.6",
    "istanbul-reports": "3.1.5",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-haste-map": "29.5.0",
    "jest-preset-angular": "13.1.1",
    "jest-resolve": "29.5.0",
    "nx": "16.2.2",
    "nx-cloud": "16.0.5",
    "rxjs": "7.8.1",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  }
}
