{
  "name": "ngflex-libraries",
  "version": "0.0.1",
  "license": "MIT",
  "description": "NGFlex ngrx, firebase and js libraries",
  "packageManager": "yarn@3.2.1",
  "author": "FlyFlex",
  "scripts": {
    "build:ngrx-firebase": "nx run ngrx-firebase:build",
    "build": "nx run-many --target=build --all",
    "lint": "nx run-many --target=lint --all",
    "test": "nx run-many --target=test --all --coverage",
    "test:merge-reports": "node tools/scripts/merge-coverage-reports.js",
    "test:watch": "jest --watch",
    "publish:ngrx-firebase": "nx run ngrx-firebase:publish",
    "verif:all": "yarn build && yarn lint && yarn test"
  },
  "dependencies": {
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@angular/fire": "7.5.0",
    "@firebase/app-types": "0.8.1",
    "@ngrx/store": "14.3.2",
    "@nrwl/cli": "15.2.1",
    "@nrwl/eslint-plugin-nx": "15.2.1",
    "@nrwl/jest": "15.2.1",
    "@nrwl/js": "15.2.1",
    "@nrwl/linter": "15.2.1",
    "@nrwl/workspace": "15.2.1",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "eslint": "8.28.0",
    "firebase": "9.14.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-haste-map": "29.3.1",
    "jest-preset-angular": "12.2.2",
    "jest-resolve": "29.3.1",
    "nx": "15.2.1",
    "rxjs": "7.5.7",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  }
}
