{
  "name": "ngflex-libs",
  "version": "0.0.1",
  "license": "MIT",
  "description": "NGFlex ngrx, firebase and js libraries",
  "packageManager": "yarn@3.2.1",
  "repository": {
    "type": "github",
    "url": "https://github.com/flyflex/ngflex-libs"
  },
  "author": "FlyFlex",
  "scripts": {
    "build:ngrx-firebase": "nx run ngrx-firebase:build",
    "build": "nx run-many --target=build --all",
    "lint": "nx run-many --target=lint --all",
    "test:affected": "nx affected --target=test --coverage",
    "test": "nx run-many --target=test --all --coverage",
    "test:watch": "jest --watch",
    "publish:ngrx-firebase": "nx run ngrx-firebase:publish --tag=next --otp",
    "publish:merge-coverage-reports": "nx run merge-coverage-reports:publish --tag=next --otp",
    "verif:all": "yarn build && yarn lint && yarn test",
    "merge-reports": "nx run merge-coverage-reports:merge"
  },
  "dependencies": {
    "minimist": "^1.2.7",
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@angular/fire": "7.5.0",
    "@firebase/app-types": "0.9.0",
    "@ngrx/store": "15.4.0",
    "@nrwl/cli": "15.8.9",
    "@nrwl/eslint-plugin-nx": "15.8.9",
    "@nrwl/jest": "15.8.9",
    "@nrwl/js": "15.8.9",
    "@nrwl/linter": "15.8.9",
    "@nrwl/workspace": "15.8.9",
    "@types/fs-extra": "11.0.1",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.0",
    "@types/minimist": "1.2.2",
    "@types/node": "18.15.10",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "eslint": "8.36.0",
    "firebase": "9.18.0",
    "fs-extra": "11.1.1",
    "glob": "9.3.2",
    "istanbul-reports": "3.1.5",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-haste-map": "29.5.0",
    "jest-preset-angular": "13.0.1",
    "jest-resolve": "29.5.0",
    "nx": "15.8.9",
    "rxjs": "7.8.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
