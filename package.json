{
  "name": "ngflex-libs",
  "version": "4.0.0-rc.1",
  "license": "MIT",
  "description": "NGFlex ngrx, firebase and js libraries",
  "packageManager": "yarn@3.6.2",
  "repository": {
    "type": "github",
    "url": "https://github.com/flyflex/ngflex-libs"
  },
  "author": "FlyFlex",
  "scripts": {
    "build:ngrx-firebase": "nx run ngrx-firebase:build",
    "build": "nx run-many --target=build --all --skipNxCache",
    "lint": "nx run-many --target=lint --all --skipNxCache",
    "test:affected": "nx affected --target=test --coverage",
    "test": "nx run-many --target=test --all --coverage --skipNxCache",
    "test:watch": "jest --watch",
    "publish:ngrx-firebase": "nx run ngrx-firebase:publish --tag=next --otp",
    "publish:merge-coverage-reports": "nx run merge-coverage-reports:publish --tag=next --otp",
    "verif:all": "yarn build && yarn lint && yarn test",
    "merge-reports": "nx run merge-coverage-reports:merge && cat coverage/report/lcov.info"
  },
  "dependencies": {
    "minimist": "1.2.8",
    "tslib": "2.6.0"
  },
  "devDependencies": {
    "@angular/fire": "16.0.0",
    "@firebase/app-types": "0.9.0",
    "@ngrx/store": "16.1.0",
    "@nx/eslint": "17.2.8",
    "@nx/eslint-plugin": "17.2.8",
    "@nx/jest": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "29.5.11",
    "@types/minimist": "1.2.5",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "eslint": "8.56.0",
    "firebase": "10.7.1",
    "fs-extra": "11.2.0",
    "glob": "10.3.10",
    "istanbul-reports": "3.1.6",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-haste-map": "29.7.0",
    "jest-preset-angular": "13.1.4",
    "jest-resolve": "29.7.0",
    "nx": "17.2.8",
    "rxjs": "7.8.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}